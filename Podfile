use_frameworks!

target 'IntuneFeatures.OSX' do
  platform :osx, '10.12'
  target 'IntuneFeaturesTests.OSX'

  pod 'HDF5Kit', :git => 'https://github.com/aleph7/HDF5Kit.git', :branch => 'master'
  pod 'Peak/MIDI', :git => 'https://github.com/hoseking/Peak.git', :branch => 'master'
  pod 'Upsurge', :git => 'https://github.com/aleph7/Upsurge.git', :branch => 'master'
end

target 'IntuneFeatures.iOS' do
  platform :ios, '10.0'
  target 'IntuneFeaturesTests.iOS'

  pod 'HDF5Kit', :git => 'https://github.com/aleph7/HDF5Kit.git', :branch => 'master'
  pod 'Peak/MIDI', :git => 'https://github.com/hoseking/Peak.git', :branch => 'master'
  pod 'Upsurge', :git => 'https://github.com/aleph7/Upsurge.git', :branch => 'master'
end

target :CompileFeatures do
  platform :osx, '10.12'

  pod 'HDF5Kit', :git => 'https://github.com/aleph7/HDF5Kit.git', :branch => 'master'
  pod 'Peak/MIDI', :git => 'https://github.com/hoseking/Peak.git', :branch => 'master'
  pod 'Peak/AudioFile', :git => 'https://github.com/hoseking/Peak.git', :branch => 'master'
  pod 'Upsurge', :git => 'https://github.com/aleph7/Upsurge.git', :branch => 'master'
end
